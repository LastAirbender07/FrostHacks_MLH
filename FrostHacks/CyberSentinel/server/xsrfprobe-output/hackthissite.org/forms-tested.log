<form action="/user/login" id="loginform" method="post">
 <div id="innerlogin">
  <script type="text/javascript">
   var userclicked=0; var passclicked=0;
  </script>
  <p>
   <input class="login" name="username" onclick="if(userclicked==0){this.value='';userclicked=1;};" title="Username" type="text" value=""/>
  </p>
  <p>
   <input class="login" name="password" onclick="if(passclicked==0){this.value='';passclicked=1;};" title="Password" type="password" value=""/>
  </p>
  <p>
   <input class="submit-button" name="btn_submit" type="submit" value="Login"/>
  </p>
 </div>
</form>



